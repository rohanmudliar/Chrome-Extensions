{
    "manifest_version": 2,
    "name": "Replace",
    "version": "0.1",
    "web_accessible_resources": [ "Dog/*.jpg" ],
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["script.js"]
        }
    ],
    
    "background": {
        "scripts": ["background.js"]
    },
    
    "browser_action": {
        "default_icon": "icon.png"
    }
}