{
    "manifest_version": 2,
    "name": "PopUp Messaging",
    "version": "0.1",
    "content_scripts": [{
        "js": ["p5/p5.js","p5/addons/p5.dom.js","sketch.js", "content.js"],
        "matches": ["<all_urls>"]
    }],
    "permissions": ["tabs"],
    "browser_action": {
        "default_icon": "icon.png",
        "default_name": "TextDefine",
        "default_popup": "index.html"
    }
}