{
    "manifest_version": 2,
    "name": "Yoo 2",
    "version": "0.1",
    "content_scripts": [
        {
            "matches": [ "<all_urls>" ],                            //OR for specific link..
            //"matches": ["http://en.wikipedia.org/*", "https://en.wikipedia.org/*"],
            "js": ["script.js"]
        }
    ],
    
    "background": {
        "scripts": ["background.js"]
    },
    
    "browser_action": {
        "default_icon": "icon.png"
    }
}