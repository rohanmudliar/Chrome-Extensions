{
    "manifest_version": 2,
    "name": "Pop Up",
    "version": "0.1",
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html",
        "default_title": "Pop Up Yoo!!"
    },
    
    "content_scripts": [{
        "js": ["script.js"],
        "matches": ["<all_urls>"]
    }]
    
}